{
    "name": "itspire/framework-extra-bundle",
    "description": "Itspire - Symfony Framework Extra Bundle",
    "type": "symfony-bundle",
    "keywords": ["itspire", "symfony", "query", "request", "header", "file", "serializer", "json", "xml"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": "~8.1",
        "ext-json": "*",
        "ext-xml": "*",
        "doctrine/common": "~3.3",
        "itspire/common": "~3.1",
        "itspire/exceptions": "~2.1",
        "jms/serializer-bundle": "~4.0",
        "psr/log": "~2.0",
        "sensio/framework-extra-bundle": "~6.2",
        "symfony/browser-kit": "~5.4",
        "symfony/cache": "~5.4",
        "symfony/dependency-injection": "~5.4",
        "symfony/dom-crawler": "~5.4",
        "symfony/error-handler": "~5.4",
        "symfony/event-dispatcher": "~5.4",
        "symfony/expression-language": "~5.4",
        "symfony/filesystem": "~5.4",
        "symfony/framework-bundle": "~5.4",
        "symfony/http-foundation": "~5.4",
        "symfony/http-kernel": "~5.4",
        "symfony/mime": "~5.4",
        "symfony/psr-http-message-bridge": "~2.1",
        "symfony/routing": "~5.4",
        "symfony/security-bundle": "~5.4",
        "symfony/twig-bundle": "~5.4",
        "symfony/web-profiler-bundle": "~5.4",
        "symfony/yaml": "~5.4"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\FrameworkExtraBundle\\": "src/main/php"
        }
    },
    "require-dev": {
        "roave/security-advisories": "dev-latest",
        "squizlabs/php_codesniffer": "~3.x-dev",
        "symfony/phpunit-bridge": "~6.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\FrameworkExtraBundle\\Tests\\TestApp\\": "src/test/php/TestApp/src",
            "Itspire\\FrameworkExtraBundle\\Tests\\": "src/test/php"
        }
    },
    "config": {
        "platform": {
            "php": "8.1"
        }
    }
}
