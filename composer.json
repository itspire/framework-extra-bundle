{
    "name": "itspire/framework-extra-bundle",
    "description": "Itspire - Symfony Framework Extra Bundle",
    "type": "symfony-bundle",
    "keywords": ["itspire", "symfony", "query", "request", "header", "file", "serializer", "json", "xml"],
    "homepage": "http://www.itspire.fr",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "Ryan RAJKOMAR",
            "email": "ryan.rajkomar@itspire.fr"
        }
    ],
    "require": {
        "php": "~7.4",
        "ext-json": "*",
        "ext-xml": "*",
        "doctrine/common": "~3.1",
        "itspire/exceptions-resolvers": "~1.0",
        "jms/serializer-bundle": "~3.7",
        "psr/log": "~1.1",
        "sensio/framework-extra-bundle": "~5.6",
        "symfony/browser-kit": "~4.4",
        "symfony/config": "~4.4",
        "symfony/expression-language": "~4.4",
        "symfony/framework-bundle": "~4.4",
        "symfony/http-foundation": "~4.4",
        "symfony/mime": "~4.4",
        "symfony/psr-http-message-bridge": "~2.0",
        "symfony/routing": "~4.4",
        "symfony/security-bundle": "~4.4",
        "symfony/twig-bundle": "~4.4",
        "symfony/web-profiler-bundle": "~4.4",
        "symfony/yaml": "~4.4"
    },
    "autoload": {
        "psr-4": {
            "Itspire\\FrameworkExtraBundle\\": "src/main/php"
        }
    },
    "require-dev": {
        "phploc/phploc": "~7.0",
        "roave/security-advisories": "dev-latest",
        "sebastian/phpcpd": "~6.0",
        "squizlabs/php_codesniffer": "~3.5",
        "symfony/phpunit-bridge": "~5.1"
    },
    "autoload-dev": {
        "psr-4": {
            "Itspire\\FrameworkExtraBundle\\Tests\\TestApp\\": "src/test/php/TestApp/src",
            "Itspire\\FrameworkExtraBundle\\Tests\\": "src/test/php"
        }
    },
    "config": {
        "platform": {
            "php": "7.4"
        }
    }
}
